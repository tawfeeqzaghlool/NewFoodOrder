<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="16dp"
    android:paddingLeft="16dp"
    android:paddingRight="16dp"
    android:paddingTop="16dp"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.admin.newfoodorder.MainActivity"
    tools:showIn="@layout/activity_main">

    <Button
        android:id="@+id/addFood"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="addFoodButtonClicked"
        android:text="@string/addFood" />

    <Button
        android:id="@+id/openOrders"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="viewOrders"
        android:text="@string/viewOrders" />

</LinearLayout>
